{
    "name": "fgt-as-handler",
    "version": "3.1.0-alpha.0",
    "description": "FortiGate Autoscale Handler function for AWS.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "tsc": "tsc -d",
        "build": "rm -rf dist/{*,**/*}.d.ts; npm run tsc;",
        "build-artifacts": "npm run build; node scripts/make-dist.js;",
        "test": "mocha -r ts-node/register tests/*.spec.ts",
        "lint-check": "node node_modules/ftnt-devops-ci/bin/ftnt-devops-ci.js c -fl  -F .eslintignore -L .eslintignore \"{*,**/*}{.js,.json,.ts}\"",
        "lint-fix": "node node_modules/ftnt-devops-ci/bin/ftnt-devops-ci.js f -fl  -F .eslintignore -L .eslintignore \"**/*{.js,.json,.ts}\""
    },
    "keywords": [],
    "author": "Fortinet <github@fortinet.com>",
    "license": "MIT",
    "dependencies": {
        "@types/aws-lambda": "^8.10.46",
        "autoscale-core": "file:../../../core",
        "http-status-codes": "^1.4.0"
    },
    "devDependencies": {
        "@types/node": "^13.7.5",
        "@typescript-eslint/eslint-plugin": "^2.21.0",
        "@typescript-eslint/parser": "^2.21.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-mocha": "^6.3.0",
        "eslint-plugin-prettier": "^3.1.2",
        "ftnt-devops-ci": "github:fortinet/ftnt-devops-ci",
        "mocha": "^7.1.0",
        "prettier": "^1.19.1",
        "shelljs": "^0.8.3",
        "sinon": "^9.0.0",
        "ts-node": "^8.6.2",
        "typescript": "^3.8.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fortinet/fortigate-autoscale-aws.git"
    },
    "bugs": {
        "url": "https://github.com/fortinet/fortigate-autoscale-aws/issues"
    },
    "homepage": "https://github.com/fortinet/fortigate-autoscale-aws#readme"
